<?xml version="1.0" encoding="utf-8"?>

<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start">


    <include
        layout="@layout/app_bar_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <androidx.coordinatorlayout.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:context=".MainActivity">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="80dp"
                android:paddingBottom="10dp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="10dp"
                    android:paddingTop="10dp"
                    android:orientation="vertical">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnBasics"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/innerbg2"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Basics"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image7"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg16"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnProbability"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg1"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Probability"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg2"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnLinearRegression"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg2"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Linear Regression"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image2"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg22"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnFrequency"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg3"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Frequency Distribution"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image3"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg10"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnNormalDistribution"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg4"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Normal Distribution"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image6"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg15"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnDistances"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg5"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Distances"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image11"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg6"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnPieChart"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/bg6"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Pie-Chart"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image12"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg23"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnChiSquare"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/innerbg4"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="Chi-Square Distribution"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image13"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg25"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>


                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        android:layout_height="100dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginBottom="30dp"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="12dp"
                        >

                        <LinearLayout
                            android:id="@+id/btnTtest"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/innerbg8"
                            android:orientation="horizontal"
                            android:padding="10dp">

                            <LinearLayout
                                android:layout_width="200dp"
                                android:layout_height="match_parent"
                                android:orientation="horizontal">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center|left"
                                    android:layout_marginLeft="2dp"
                                    android:text="T test - One Sample"
                                    android:fontFamily="@font/my_nunito"
                                    android:textColor="#E4E5EC"
                                    android:textColorHint="#FBF1F5"
                                    android:textColorLink="#F3ECEC"
                                    android:textSize="20sp"
                                    android:textStyle="bold" />

                            </LinearLayout>

                            <RelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <de.hdodenhof.circleimageview.CircleImageView
                                    android:id="@+id/profile_image4"
                                    android:layout_width="96dp"
                                    android:layout_height="96dp"
                                    android:layout_alignParentRight="true"
                                    android:padding="2dp"
                                    android:layout_marginRight="5dp"
                                    android:src="@drawable/primg11"
                                    app:civ_border_color="#FF000000"
                                    app:civ_border_width="2dp" />

                            </RelativeLayout>


                        </LinearLayout>
                    </androidx.cardview.widget.CardView>






                </LinearLayout>
            </androidx.core.widget.NestedScrollView>

        </LinearLayout>


    </androidx.coordinatorlayout.widget.CoordinatorLayout>


    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/nav_header_main"
        app:menu="@menu/activity_main_drawer" />
</androidx.drawerlayout.widget.DrawerLayout>




