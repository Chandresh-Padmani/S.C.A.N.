<?xml version="1.0" encoding="utf-8"?>

<androidx.drawerlayout.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:openDrawer="start">


    <androidx.coordinatorlayout.widget.CoordinatorLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".EqBasic"
        android:background="@drawable/modulebg1">

        <include
            layout="@layout/app_bar_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="80dp"
            android:padding="20dp"
            >

            <RelativeLayout
                android:id="@+id/rl1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                >

                <RelativeLayout
                    android:id="@+id/rlayout1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Mean"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout1"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/mean1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv1"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        android:layout_below="@id/tv1"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/mean_a" />

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv2"
                        android:layout_below="@id/iv1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/mean2"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/rlayout3"
                    android:layout_below="@id/rlayout2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Median"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout3"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv3"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/median1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv2"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        android:layout_below="@id/tv3"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/median1" />

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv4"
                        android:layout_below="@id/iv2"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/median2"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv3"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        android:layout_below="@id/tv4"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/median2" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout5"
                    android:layout_below="@id/rlayout4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Mode"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout5"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv5"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/mode1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv4"
                        android:layout_width="wrap_content"
                        android:layout_height="70dp"
                        android:layout_below="@id/tv5"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/mode" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout7"
                    android:layout_below="@id/rlayout6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Standard Deviation"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout7"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv6"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/stdev1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv5"
                        android:layout_width="wrap_content"
                        android:layout_height="250dp"
                        android:layout_below="@id/tv6"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/st_dev" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout9"
                    android:layout_below="@id/rlayout8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Variance"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout9"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv8"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/variance1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv7"
                        android:layout_width="wrap_content"
                        android:layout_height="250dp"
                        android:layout_below="@id/tv8"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/variance" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout11"
                    android:layout_below="@id/rlayout10"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Co-efficient of Variance"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout11"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv10"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/coeff1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv9"
                        android:layout_width="wrap_content"
                        android:layout_height="200dp"
                        android:layout_below="@id/tv10"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/coeff_var" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout13"
                    android:layout_below="@id/rlayout12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Geometric Mean"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout13"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv12"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/geomean1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv11"
                        android:layout_width="wrap_content"
                        android:layout_height="200dp"
                        android:layout_below="@id/tv12"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/geomean" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout15"
                    android:layout_below="@id/rlayout14"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp">

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="Percentile"
                        android:fontFamily="@font/balsamiqsans_bold"
                        android:textSize="28sp"
                        android:gravity="center"
                        android:textColor="#8A0B0B"
                        />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlayout16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/rlayout15"
                    android:layout_margin="8dp">

                    <com.codesgood.views.JustifiedTextView
                        android:id="@+id/tv14"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="5dp"
                        android:text="@string/percentile1"
                        android:fontFamily="@font/kumbhsans_regular"
                        android:textSize="16sp"
                        android:textColor="#000000"/>

                    <ImageView
                        android:id="@+id/iv13"
                        android:layout_width="wrap_content"
                        android:layout_height="200dp"
                        android:layout_below="@id/tv14"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentEnd="true"
                        android:layout_alignParentRight="true"
                        android:layout_marginStart="42dp"
                        android:layout_marginLeft="42dp"
                        android:layout_marginBottom="8dp"
                        android:layout_marginTop="5dp"
                        android:layout_marginEnd="47dp"
                        android:layout_marginRight="47dp"
                        android:foregroundGravity="center"
                        android:src="@drawable/pr1" />

                </RelativeLayout>

            </RelativeLayout>

        </ScrollView>

    </androidx.coordinatorlayout.widget.CoordinatorLayout>


    <com.google.android.material.navigation.NavigationView
        android:id="@+id/nav_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/nav_header_main"
        app:menu="@menu/activity_main_drawer" />
</androidx.drawerlayout.widget.DrawerLayout>

